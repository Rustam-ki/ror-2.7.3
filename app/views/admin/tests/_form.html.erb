<%= form_with model: [:admin, @test], local: true do |form| %>
  <%= render 'shared/errors', resource: @test %>

  <p>
    <%= form.label :title %>
    <%= form.text_field :title, id: :test_title %>
  </p>

  <p>
    <%= form.label :level %>
    <%= form.text_field :level, id: :test_level %>
  </p>

  <p>
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>

  <p>
    <%= form.label :timer %>
    <%= form.number_field :timer, id: :timer, min: 1, max: 30, step: 1 %>
  </p>

  <p>
    <%= form.label :completed %>
    <%= form.check_box :completed, id: :test_completed %>
  </p>



  <p><%= form.submit %></p>
<% end %>